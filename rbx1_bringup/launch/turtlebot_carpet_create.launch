<launch>

  <param name="/use_sim_time" value="false" />

  <!-- Calibration parameters after calibration of an original Create TurtleBot on low-ply carpet -->
  <param name="turtlebot_node/gyro_scale_correction" value="1.42"/>
  <param name="turtlebot_node/odom_angular_scale_correction" value="0.965"/>
  <param name="turtlebot_node/odom_linear_scale_correction" value="0.954"/>

  <include file="$(find rbx1_bringup)/launch/turtlebot_minimal_create.launch" />

</launch>
